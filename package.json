{
    "name": "consensus",
    "author": "Your Name",
    "version": "1.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "pnpm -r dev",
        "dev:server": "pnpm --filter server dev",
        "dev:frontend": "pnpm --filter frontend dev",
        "dev:all": "pnpm dev:server & pnpm dev:frontend",
        "build": "pnpm -r build",
        "build:server": "pnpm --filter server build",
        "build:frontend": "pnpm --filter frontend build",
        "type-check": "pnpm -r exec tsc --noEmit",
        "type-check:server": "pnpm --filter server exec tsc --noEmit",
        "type-check:frontend": "pnpm --filter frontend exec tsc --noEmit",
        "test": "vitest",
        "preview": "pnpm --filter frontend preview"
    },
    "dependencies": {
        "uuid": "^13.0.0",
        "yaml": "^2.8.2",
        "zod": "^4.3.6"
    },
    "devDependencies": {
        "@types/node": "^20.19.30",
        "rollup-plugin-node-externals": "^8.1.2",
        "tsx": "^4.21.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.18"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "cpu-features",
            "esbuild",
            "protobufjs",
            "ssh2"
        ],
        "overrides": {
            "react": "^19.2.3",
            "react-dom": "^19.2.3"
        }
    }
}
